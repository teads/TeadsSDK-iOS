target 'TeadsSampleApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'TeadsSDK', '~> 5.1.3'

  pod 'TeadsSASAdapter', '~> 5.1.3'
  pod 'TeadsAdMobAdapter', '~> 5.1.3'
  pod 'TeadsAppLovinAdapter', '~> 5.1.3'
  
  pod 'SwiftFormat/CLI'
end
